class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        int n=nums.size();
        int i=0;
        bool flag=false;
        sort(nums.begin(),nums.end());
        for(i=0;i<n;i++){
            if(nums[i]==nums[i+1]){
                flag=true;
                }
            
            
           
        }

    return flag;   
    }
};
