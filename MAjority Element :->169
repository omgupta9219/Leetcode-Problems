class Solution {
    public int majorityElement(int[] nums) {
        int freq=0;
        int ans=0;
        for(int val:nums){
            if(freq==0){
                ans=val;
            }
            if(ans==val){
                freq++;
                
            }
            else{
                freq--;
            }
        }
    return ans;    
    }
}
