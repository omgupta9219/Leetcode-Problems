class Solution {
    public int[] getNoZeroIntegers(int n) {
        for(int i=1;i<=n;i++){
            int first=i;
            int second=n-i;
            if(noZero(first) && noZero(second)){
                return new int[]{first,second};
            }
        }
    return new int[]{-1,-1};    
    }
    public boolean noZero(int x){
        while(x>0){
            int digit=x%10;
            if(digit%10==0)return false;
            x=x/10;
        }
    return true;
    }
}
